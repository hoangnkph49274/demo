<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Update Uesr : <%= data.name%></title>
</head>
<body>
    <h1>Update Uesr : <%= data.name%></h1>
    <form action="/updateUser" method="post" enctype="application/x-www-form-urlencoded">
        <input name="id" value="<%= data.id %>" hidden>
        <input name="name" value="<%= data.name %>">
        <input name="address" value="<%= data.address %>">
        <input name="phone" value="<%= data.phone %>">
        <button type="submit">Update</button>
    </form>
</body>
</html>